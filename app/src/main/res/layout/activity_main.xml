<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="77dp"
        android:layout_height="53dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="234dp"
        android:contentDescription="TODO"
        app:srcCompat="@drawable/logotecsup" />

    <ImageView
        android:id="@+id/imagenPolideportivoId"
        android:layout_width="wrap_content"
        android:layout_height="151dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="57dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="208dp"
        android:contentDescription="TODO"
        android:scaleType="centerCrop"
        android:src="@drawable/laboratorios"
        app:srcCompat="@drawable/laboratorios"
        tools:ignore="UnknownId,ImageContrastCheck" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="140dp"
        android:onClick="irAReservaLaboratorios"
        android:text="@string/reserva_laboratorios" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="308dp"
        android:layout_marginEnd="35dp"
        android:onClick="irAReservaPolideportivos"
        android:text="@string/reserva_polideportivos" />

    <ImageView
        android:id="@+id/imagenLaboratoriosId"
        android:layout_width="wrap_content"
        android:layout_height="157dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="133dp"
        android:layout_marginEnd="51dp"
        android:scaleType="centerCrop"
        android:src="@drawable/polideportivo"
        app:srcCompat="@drawable/polideportivo"
        tools:ignore="UnknownId" />



    <ImageView
        android:id="@+id/logoId"
        android:layout_width="55dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="345dp"
        android:layout_marginBottom="22dp"
        android:contentDescription="@string/todo"
        app:srcCompat="@drawable/logotecsup" />

    <View
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="77dp"
        android:background="@drawable/sombreado_linea"
        android:padding="8dp"
        tools:ignore="UnknownId" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="102dp"
        android:background="@drawable/sombreado_linea"
        android:padding="8dp"
        tools:ignore="UnknownId" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="167dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="148dp"
        android:text="@string/tecsite"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="197dp"
        android:layout_marginBottom="31dp"
        android:text="@string/tecsite_2023"
        android:textSize="20sp" />

</RelativeLayout>
